void loop(){
  WiFiClient client = server.available();
  if (client) {
    String currentLine = "";
    while (client.connected()) {
      if (client.available()) {
        char c = client.read();
        Serial.write(c);
        if (c == '\n') {
          if (currentLine.length() == 0) {
            client.println("HTTP/1.1 200 OK");
            client.println("Content-type:text/html");
            client.println();
            client.print(html);
            client.println();
            for(int i=0; i<7; i++){
              for(uint16_t count=0; count<30; count++){
                digitalWrite(spIn, HIGH);
                delayMicroseconds(460);
                digitalWrite(spIn, LOW);
                delayMicroseconds(460);
              }
              Serial.write(i);
              delay(50);
            }
            break;
            } else {
            currentLine = "";
          }
        } else if (c != '\r') {
          currentLine += c;
        }
        if (currentLine.startsWith("GET /?key=")) {
          // do nothing
          break;
        }else if (currentLine.endsWith("GET /is_exists")) {
          client.println("HTTP/1.1 200 OK");
          client.println("Content-type:text/plain");
          client.println();
          client.println(true);
          for(int i=0; i<7; i++){
            for(uint16_t count=0; count<30; count++){
              digitalWrite(spIn, HIGH);
              delayMicroseconds(460);
              digitalWrite(spIn, LOW);
              delayMicroseconds(460);
            }
            Serial.write(i);
            delay(50);
          }
          break;
        }
      }
    }
    client.stop();
  }
}

